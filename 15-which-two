public String whichTwo(Integer a, Integer b, Integer c) {
    //code here
    Integer aResult = b+c;
    Integer bResult = a+c;
    Integer cResult = a+b;

    if(aResult == a){
        return 'a';
    } else if(bResult == b) {
        return 'b';
    } else if (cResult == c){
        return 'c';
    } else{
        return '';
    }
}