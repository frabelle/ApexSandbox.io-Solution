public Boolean isLeapYear(Integer year) {
    //code here

    if(Math.mod(year,100) == 0){
        if(Math.mod(year,4) == 0 && Math.mod(year,400)){
            return true;
        }else{
            return false;
        }
    } else if (Math.mod(year,4) == 0) {
        return true;
    } else{
        return false;
    }
}
